{
  "presets": ["react", "es2015"],
  "plugins": [
    "transform-object-assign",
    "transform-flow-strip-types",
    "transform-runtime",
    "transform-es2015-modules-umd"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "react-transform",
          {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": ["react"],
                "locals": ["module"]
              }
            ]
          }
        ]
      ]
    }
  }
}
